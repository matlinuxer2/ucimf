################################################################################
#
#	jfbterm 設定ファイル	
#
################################################################################

################################################################################
#
#	その他の指定
#
################################################################################
#	仮想コンソールを新しく取得するかの指定
#	0	...	可能なら取得しない。
#	1	...	取得する。
newvc		:	0
#	仮想コンソールを新しく取得した場合、utmp に登録するかの指定
utmp		:	1
#	環境変数 TERM に設定する値(デフォルトは TERM=jfbterm)
term		:	jfbterm


################################################################################
#
#	フォントの指定
#	絶対パス or PWDからの相対パス
#
################################################################################
fontset : iso646-1973irv,pcf,L,/usr/X11R6/lib/X11/fonts/misc/8x16.pcf.gz
+fontset: ansix3.4-1968,alias,L,iso646-1973irv
